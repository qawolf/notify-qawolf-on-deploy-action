{
  "name": "@qawolf/notify-qawolf-on-deploy-action",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "engines": {
    "node": "^16 || ^18 || ^20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:clean": "tsc --build --clean && rm -rf ./dist",
